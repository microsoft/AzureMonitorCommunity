                "formatter": 6
              },
              {
                "columnMatch": "lastSkuUpdate",
                "formatter": 6
              },
              {
                "columnMatch": "dataIngestionStatus",
                "formatter": 5
              }
            ],
            "labelSettings": [
              {
                "columnId": "id",
                "label": "Workspace"
              },
              {
                "columnId": "location",
                "label": "Location"
              },
              {
                "columnId": "resourceGroup",
                "label": "Resource Group"
              },
              {
                "columnId": "createdDate",
                "label": "Created Date"
              },
              {
                "columnId": "retentionInDays",
                "label": "Retention in Days"
              },
