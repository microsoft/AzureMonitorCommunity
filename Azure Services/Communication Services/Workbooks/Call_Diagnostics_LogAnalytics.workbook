            "content": {
              "version": "KqlItem/1.0",
              "query": "ACSCallSummary\r\n| where CallStartTime {call_time_range:query}\r\n| distinct CorrelationId, CallStartTime, CallType\r\n| extend time_span = floor(CallStartTime, {call_time_granularity})\r\n| make-series event_count=count() default=0 on CallStartTime from {call_time_range:start} to {call_time_range:end} step {call_time_granularity} by {call_grouping}\r\n| sort by  {call_grouping} asc\r\n| render {plot_type} // title=\"Number of calls by {call_grouping}\"",
              "size": 0,
              "timeContext": {
                "durationMs": 2592000000
              },
